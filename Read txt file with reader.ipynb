    "lst = list()\n",
    "for line in fhand:\n",
    "    words = line.rstrip().split()\n",
    "    for word in words:\n",
    "        if word in lst:\n",
    "            continue\n",
    "        if word not in lst:\n",
    "            lst.append(word) \n",
    "            lst.sort()"
